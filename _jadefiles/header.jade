mixin nav-content()
	ul
		li.one: a(href="#about") About
		li.two: a(href="#horatiu") Horatiu
		li.three: a(href="#crama") Crama
		li.four: a(href="#Contact") Contact

.mobile-navigation
	+nav-content()


header
	.header-position
		h1.logo GrammaWines

		nav.site-nav
			+nav-content()

		a(href="mailto:office@grammawines.ro").email-link +emailus

		.mobile-navigation-toggle
			.bar

